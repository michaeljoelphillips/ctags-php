{
    "name": "michaeljoelphillips/ctags-php",
    "description": "Reads tag files generated by universal ctags",
    "type": "library",
    "keywords": [
        "universal",
        "ctags"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpbench/phpbench": "^0.17.1",
        "doctrine/coding-standard": "^8.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Phillips",
            "email": "michaeljoelphillips@gmail.com"
        }
    ],
    "require": {
        "nikic/php-parser": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "CTags\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CTags\\Tests\\": "tests/"
        }
    }
}
